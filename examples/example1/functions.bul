/* Returns the sum of a and b */
func sum(a: i64, b: i64) -> i64 {
  return a + b
}

/* Returns the product of a and b */
func product(a: i64, b: i64) -> i64 {
  return a * b
}

func main() -> i64 {
  return sum(5, 6)
}
