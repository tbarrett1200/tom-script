func factorial(num: Integer) -> Integer {
  if num < 2 {
    return 1
  } else {
    return num * factorial(num-1)
  }
}

func super_factorial(num: Integer) -> Integer {
 if num < 2 {
  return 1
 } else {
  return factorial(num) * super_factorial(num-1)
 }
}

func pow(a: Integer, b: Integer) -> Integer {
 if b == 0 {
  return 1
 } else if b < 2 {
  return a
 } else {
  return a * pow(a, b-1)
 }
}

func hyper_factorial(num: Integer) -> Integer {
 if num == 1 {
  return 1
 } else {
  return pow(num, num) * hyper_factorial(num-1)
 }
}
