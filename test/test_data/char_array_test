extern func puts(*Character) -> Int
extern func putchar(Character) -> Int
extern func strlen(*Character) -> Int
extern func strcmp(*Character, *Character) -> Int

func main() -> Boolean {
  var string1: [Character, 12] = "Hello World"
  var string2: [Character, 12] = "Hello World"

  puts(&string1.0)
  puts(&string2.0)

  return strcmp(&string1.0, &string2.0) == 0
}
